{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
  "rules": {
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "jsx-quotes": ["error", "prefer-single"],
    "no-tabs": 0,
    "indent": ["error", "tab"],
    "react/jsx-indent": [2, "tab"],
    "react/jsx-indent-props": [0, 'tab'],
    "react/destructuring-assignment": ["error", "never"],
    "func-names": ["error", "as-needed"]
  }
}
